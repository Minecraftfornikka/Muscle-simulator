-- Scripts:

local function NHWIOY_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	frame = script.Parent
	frame.Draggable = true
	frame.Selectable = true
	frame.Active = true
	
	print("Loaded!")
end
coroutine.wrap(NHWIOY_fake_script)()
local function MKITDS_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	wait()
	if game:IsLoaded() then
		local Turned = false
		local Playerr = game:WaitForChild("Players").LocalPlayer.Name
	
	
		script.Parent.Activated:Connect(function()
			if Turned == false then
				Turned = true
				script.Parent.BackgroundColor3 = Color3.new(0.14902, 1, 0)
				while Turned == true do
					game.ReplicatedStorage.addStrength:FireServer(workspace:WaitForChild(Playerr).Weight.Handle, 1)
					wait(.01)
				end		
			else
				Turned = false
				script.Parent.BackgroundColor3 = Color3.new(255, 0, 0)
			end
		end)
	end
end
coroutine.wrap(MKITDS_fake_script)()
local function MPPWOO_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	wait()
	if game:IsLoaded() then
		local Turned = false
		local Playerr = game:WaitForChild("Players").LocalPlayer.Name
	
	
		script.Parent.Activated:Connect(function()
			if Turned == false then
				Turned = true
				script.Parent.BackgroundColor3 = Color3.new(0.14902, 1, 0)
				while Turned == true do
					game.ReplicatedStorage.addStrength:FireServer(workspace:WaitForChild(Playerr)["Double Weight"].Handle, 1)
					wait(.01)
				end
			else
				Turned = false
				script.Parent.BackgroundColor3 = Color3.new(255, 0, 0)
			end
		end)
	end
end
coroutine.wrap(MPPWOO_fake_script)()
local function MNEMF_fake_script() -- Check.LocalScript 
	local script = Instance.new('LocalScript', Check)

	script.Parent.Activated:Connect(function()
		if script.Parent.Parent.KeyType.Text == "" then
			script.Parent.Parent.Visible = false
			script.Parent.Parent.Parent.Main.Visible = true
			local sfx = Instance.new("Sound", script.Parent.Parent.Parent)
			sfx.SoundId = "rbxassetid://3422389728"
			sfx:Play()
			wait(3)
			sfx:Remove()
			else
			script.Parent.Parent.text.Text = "mf put the wrong key"
			script.Parent.Parent.text.TextColor3 = Color3.new(1, 0, 0.0156863)
			local sfx = Instance.new("Sound", script.Parent.Parent.Parent)
			sfx.SoundId = "rbxassetid://3422389728"
			sfx:Play()
			wait(3)
			sfx:Remove()	
		wait(1.5)
			script.Parent.Parent.text.Text = "key: empty! KEY EMPTY there is no key"
			script.Parent.Parent.text.TextColor3 = Color3.new(255, 255, 255)
		end
	end)
end
coroutine.wrap(MNEMF_fake_script)()
local function MAHYIO_fake_script() -- KeySystem.LocalScript 
	local script = Instance.new('LocalScript', KeySystem)

	frame = script.Parent
	frame.Draggable = true
	frame.Selectable = true
	frame.Active = true
end
coroutine.wrap(MAHYIO_fake_script)()
